<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(\w)*\.(?:jpg|jpeg|png|gif|bmp|webp) pcpicapi/index.php?id=$1 [L]
    RewriteRule ^(\w)*\.(?:jpg|jpeg|png|gif|bmp|webp) mopicapi/index.php?id=$1 [L]
</IfModule>
